{
    "name": "gogo/laravel-getui",
    "description": "一个适用于laravel和lumen框架的个推push推送服务包",
    "type": "library",
    "require": {
        "illuminate/container": "^5.8",
        "php": ">=7.1.3",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "vlucas/phpdotenv": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3",
        "mockery/mockery": "^0.9.9"
    },
    "autoload": {
        "psr-4": {
            "Gogo\\GeTui\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gogo\\GeTui\\Tests\\": "./tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "gogo",
            "email": "779322720@qq.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Gogo\\GeTui\\GeTuiServiceProvider"
            ],
            "aliases": {
                "GeTui": "Gogo\\GeTui\\Facade\\GeTui"
            }
        }
    },
    "minimum-stability":"dev"
}
